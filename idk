   Import("BitzUtils", "CrimianlityHitboxExtender.lua")()
end
      		print("button pressed")
  	end    
})
Tab:AddButton({
	Name = "Infinite Stamina",
	Callback = function()oldStamina =
			hookfunction(
				getupvalue(getrenv()._G.S_Take, 2),
				function(v1, ...)
					if (Settings.InfiniteStamina) then -- god damn the roblox engine is so shit
					v1 = 0
				end
					return oldStamina(v1, ...)
				end
			)
      		print("button pressed")
  	end    
})
Tab:AddButton({
	Name = "Melee God Mode",
	Callback = function()loadstring(game:HttpGet("https://raw.githubusercontent.com/Tobias020108Back/YBA-AUT/main/Criminality-Semi-Godmode.lua"))() 
      		print("button pressed")
  	end    
