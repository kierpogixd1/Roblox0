})
Tab:AddButton({
	Name = "Hitbox!",
	Callback = function()--BROUGHT TO YOU BY RSCRIPTS.NET--

local Import = function(rep, file)
   return loadstring(game:HttpGet(("https://raw.githubusercontent.com/NougatBitz/%s/main/%s"):format(rep, file)))
